{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: DeepReversiKitPlayTest",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/server.py",
            "cwd": "${workspaceFolder}",
            "env": {
                "FLASK_APP": "server.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "--no-debugger",
                "--no-reload"
            ],
            "console": "integratedTerminal"
        }
    ]
}